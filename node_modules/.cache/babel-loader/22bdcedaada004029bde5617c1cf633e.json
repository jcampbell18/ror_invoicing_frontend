\">\n    <h6>Companies</h6>\n    <ul>\n      <li v-for=\"header in headers\" :key=\"header\">\n        <p class=\"heading\">{{header}}</p>\n      </li>\n    </ul>\n    <ul class=\"ul-lines\" v-for=\"company in companies\" :key=\"company.id\">\n      <li>\n        <p>{{company.name}}</p>\n      </li>\n      <li>\n        <p>{{company.address}}, {{company.city}}, {{company.state}} {{company.zipcode}}</p>\n      </li>\n      <li>\n        <p>{{company.website_url}}</p>\n      </li>\n      <li>\n        <a href=\"\">\n          <img src=\"@/assets/icons/preview_black_24dp.svg\" alt=\"View\" title=\"View\"/>\n        </a>\n      </li>\n    </ul>\n  </section>\n</template>\n\n<script>\nexport default {\n  name: \"Companies\",\n  props: {},\n  data: () => ({\n    companies: [],\n    headers: [\n      'Business Name',\n      'Address',\n      'Website',\n      'View'\n    ],\n    loading: false\n  }),\n  created () {\n    this.getCompanies()\n  },\n  watch: {\n\n  },\n  methods: {\n    getCompanies() {\n      this.loading = true\n\n      fetch('http://localhost:3000/api/v1/companies')\n        .then(response => response.json())\n        .then(response => {\n          this.loading = false\n          this.companies = response\n          console.log(\"Data: \", this.companies)\n        })\n        .catch(err => {\n          console.log('Fetch Error: ', err);\n        })\n    }\n  }\n}\n</script>\n\n<style>\nsection.companies {\n  grid-row: 2;\n  grid-column: 1 /  span 2;\n  width: 100%;\n}\n\nsection.companies ul {\n  list-style: none;\n\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(45px, 1fr));\n  justify-items: center;\n  align-items: center;\n}\n\nsection.companies ul li p {\n  overflow: hidden;\n}\n\nsection.companies ul.ul-lines:nth-child(odd) {\n  background: var(--primary-color);\n}\n\nsection.companies ul li {\n  width: 100%;\n  display: flex;\n  flex-flow: row wrap;\n  justify-content: center;\n  align-items: center;\n}\n\nsection.companies ul li img {\n  width: 32px;\n}\n\nsection.companies ul li a img {\n  width: 32px;\n  padding: 0.2em 0;\n  margin: 0 auto;\n  display: block;\n}\n\nsection.companies ul li input {\n  justify-self: center;\n  align-self: center;\n}\n</style>"],"sourceRoot":"src/components/companies"}]}