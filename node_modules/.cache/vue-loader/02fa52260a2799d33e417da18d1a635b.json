port default {\n  name: 'Companies',\n  data: () => ({\n    headers: [\n      'Business Name',\n      'Address',\n      'Website',\n      'View'\n    ],\n    companies: [],\n  }),\n  apollo: {\n    companies: GET_ALL_COMPANIES_QUERY\n  },\n}\n</script>\n\n<style>\nsection.companies {\n  grid-row: 2;\n  grid-column: 1 /  span 2;\n  width: 100%;\n}\n\nsection.companies ul {\n  list-style: none;\n\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(45px, 1fr));\n  justify-items: center;\n  align-items: center;\n}\n\nsection.companies ul li p {\n  overflow: hidden;\n}\n\nsection.companies ul.ul-lines:nth-child(odd) {\n  background: var(--primary-color);\n}\n\nsection.companies ul li {\n  width: 100%;\n  display: flex;\n  flex-flow: row wrap;\n  justify-content: center;\n  align-items: center;\n}\n\nsection.companies ul li img {\n  width: 32px;\n}\n\nsection.companies ul li a img {\n  width: 32px;\n  padding: 0.2em 0;\n  margin: 0 auto;\n  display: block;\n}\n\nsection.companies ul li input {\n  justify-self: center;\n  align-self: center;\n}\n</style>"]}]}